    "props": {
      "id": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The ID of this component, used to identify dash components in callbacks.\nThe ID needs to be unique across all of the components in an app."
      },
      "viewportStyle": {
        "type": {
          "name": "exact",
          "value": {
            "width": {
              "name": "string",
              "required": false
            },
            "height": {
              "name": "string",
              "required": false
            }
          }
        },
        "required": false,
        "description": "The height and the width (in px) of the container\nin which the molecules will be displayed.\nDefault: width:1000px / height:500px\nIt should be in JSON format.",
        "defaultValue": {
          "value": "{\n  width: '500x',\n  height: '500px'\n}",
          "computed": false
        }
      },
      "stageParameters": {
        "type": {
          "name": "exact",
          "value": {
            "quality": {
              "name": "string",
              "required": false
            },
            "backgroundColor": {
              "name": "string",
              "required": false
            },
            "cameraType": {
              "name": "string",
              "required": false
            }
          }
        },
        "required": false,
        "description": "Parameters (in JSON format) for the stage object of ngl.\nCurrently implemented are the quality of the visualisation\nand the background colorFor a full list see:\nhttp://nglviewer.org/ngl/api/file/src/stage/stage.js.html",
        "defaultValue": {
          "value": "{\n  quality: 'medium',\n  backgroundColor: 'white',\n  cameraType: 'perspective'\n}",
          "computed": false
        }
      },
      "imageParameters": {
        "type": {
          "name": "exact",
          "value": {
            "antialias": {
              "name": "bool",
              "required": false
            },
            "transparent": {
              "name": "bool",
              "required": false
            },
            "trim": {
              "name": "bool",
              "required": false
            }
          }
        },
        "required": false,
        "description": "Parameters (in JSON format) for exporting the image",
        "defaultValue": {
          "value": "{\n  antialias: true,\n  transparent: true,\n  trim: true\n}",
          "computed": false
        }
      },
      "downloadImage": {
        "type": {
          "name": "bool"
        },
        "required": false,
        "description": "flag if download image was pressed"
      },
      "pdbString": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Variable which defines how many molecules should be shown and/or which chain\nThe following format needs to be used:\npdbID1.chain:start-end_pdbID2.chain:start-end\n. indicates that only one chain should be shown\n: indicates that a specific range should be shown (e.g. 1-50)\n _ indicates that more than one protein should be shown"
      },
      "data": {
        "type": {
          "name": "arrayOf",
          "value": {
            "name": "exact",
            "value": {
              "filename": {
                "name": "string",
                "required": true
              },
              "ext": {
                "name": "string",
                "required": true
              },
              "selectedValue": {
                "name": "string",
                "required": true
              },
              "chain": {
                "name": "string",
                "required": true
              },
              "range": {
                "name": "string",
                "required": true
              },
              "color": {
                "name": "string",
                "required": true
              },
              "config": {
                "name": "exact",
                "value": {
                  "type": {
                    "name": "string",
                    "required": true
                  },
                  "input": {
                    "name": "string",
                    "required": true
                  }
                },
                "required": false
              },
              "resetView": {
                "name": "bool",
                "required": true
              },
              "uploaded": {
                "name": "bool",
                "required": true
              }
            }
          }
        },
        "required": false,
        "description": "The data (in JSON format) that will be used to display the molecule\nfilename: name of the used pdb/cif file\next: file extensions (pdb or cif)\nselectedValue: pdbString\nmolStyle: selected molecule representation (cartoon, stick, sphere)\nchain: selected chain\nrange: selected range\ncolor: color in hex format\nconfig.input: content of the pdb file\nconfig.type: format of config.input\nresetView: bool if the view should be resettet\nuploaded: bool if the file was uploaded",
        "defaultValue": {
          "value": "[{\n  uploaded: true,\n  selectedValue: 'placeholder',\n  resetView: false,\n  chain: 'ALL',\n  range: 'ALL',\n  color: 'red',\n  filename: 'placeholder',\n  ext: '',\n  config: {\n    type: 'text/plain',\n    input: ''\n  }\n}]",
          "computed": false
        }
      },
      "molStyle": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Variable for changing the molecule representation\nSelection of possible molecule styles:\n    'ball+stick','cartoon','licorice',\n    'line','ribbon','rope','spacefill',\n    'surface','trace','tube'",
        "defaultValue": {
          "value": "'cartoon'",
          "computed": false
        }
      }
    }
  }
}